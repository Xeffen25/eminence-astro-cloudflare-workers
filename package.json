	},
	"devDependencies": {
		"@types/node": "^25.1.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.8.1",
		"prettier-plugin-astro": "^0.14.1",
		"wrangler": "^4.62.0"
